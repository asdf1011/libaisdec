<protocol>
    <common>
        <sequence name="extended class b equipment position report">
            <field length="6" type="integer" value="19" />
            <field name="repeat indicator" length="2" type="integer" />
            <reference name="user id" type="mmsi" />
            <field name="spare:" length="8" />
            <reference name="position report" />
            <field name="spare:" length="4" />
            <sequence name="name" length="120">
                <reference name="name" type="6 bit ascii" />
            </sequence>
            <reference name="type of ship" />
            <sequence name="dimensions of ship">
                <choice name="a">
                    <field name="a 511m or greater" length="9" type="integer" value="511" />
                    <field name="a" length="9" type="integer" />
                </choice>
                <choice name="b">
                    <field name="b 511m or greater" length="9" type="integer" value="511" />
                    <field name="b" length="9" type="integer" />
                </choice>
                <choice name="c">
                    <field name="c 63m or greater" length="6" type="integer" value="63" />
                    <field name="c" length="6" type="integer" />
                </choice>
                <choice name="d">
                    <field name="d 63m or greater" length="6" type="integer" value="63" />
                    <field name="d" length="6" type="integer" />
                </choice>
            </sequence>
            <reference name="type of electronic position fixing" />
            <reference name="raim" />
            <choice name="data terminal ready">
                <field name="available" length="1" value="0" />
                <field name="not available" length="1" value="1" />
            </choice>
            <reference name="assigned mode flag" />
            <field name="spare:" length="4" />
        </sequence>
    </common>
</protocol>

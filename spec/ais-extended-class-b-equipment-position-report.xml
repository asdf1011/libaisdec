<protocol>
    <common>
        <sequence name="extended class b equipment position report">
            <field length="6" type="integer" value="19" />
            <field name="repeat indicator" length="2" type="integer" />
            <reference name="user id" type="mmsi" />
            <field name="spare:" length="8" />
            <reference name="position report" />
            <field name="spare:" length="4" />
            <sequence name="name" length="120">
                <reference name="name" type="6 bit ascii" />
            </sequence>
            <reference name="type of ship" />
            <reference name="dimensions of ship" />
            <reference name="type of electronic position fixing" />
            <reference name="raim" />
            <reference name="data terminal ready" />
            <reference name="assigned mode flag" />
            <field name="spare:" length="4" />
        </sequence>
    </common>
</protocol>
